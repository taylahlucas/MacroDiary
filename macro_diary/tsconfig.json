{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "moduleSuffixes": [".android", ".ios", ""],
    "baseUrl": "./",
    "paths": {
      "react": ["./node_modules/@types/react"],
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@mock/*": ["src/mock/*"],
      "@navigation/*": ["src/navigation/*"],
      "@redux/*": ["src/redux/*"],
      "@screens/*": ["src/screens/*"],
      "@styles/*": ["src/styles/*"],
      "@utils/*": ["src/utils/*"],
    }
  }
}
